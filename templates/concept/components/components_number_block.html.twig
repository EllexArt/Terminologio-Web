{% for component in concept.components %}
    <div id="componentNumber{{ component.number }}" style="
        width: 10%;
        height: 10%;
        position: absolute;
        top: calc({{ component.positionY }}% - 5%);
        left: calc({{ component.positionX }}% - 5%);
        ">
        <svg width="100%" height="100%">
            <ellipse rx="45%" ry="45%" cx="50%" cy="50%" fill="white" stroke="black"/>
            <text x="50%" y="50%" dominant-baseline="middle" text-anchor="middle">{{ component.number }}</text>

        </svg>
    </div>
{% endfor %}